    "require-dev": {
        "drush/drush": "^10",
        "drupal/console": "~1.0"
    },
    "replace": {
        "h5p/h5p-core": "*",
        "h5p/h5p-editor": "*"
    },
    "config": {
        "sort-packages": false,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/console-extend-plugin": true,
            "topfloor/composer-cleanup-vcs-dirs": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/libraries/ckeditor": [
                "type:drupal-library-ckeditor"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": "true",
		"patches": {
			"drupal/opigno_dashboard": {
				"login submit issue": "https://www.drupal.org/files/issues/2022-03-08/opigno_dashboard-fix-error-produced-by-missing-homepage-during-login-2.patch"
			},

			"drupal/quiz": {
				"adding filters in quiz entitylist page": "./patches/quiz.patch",
                "Question Label change": "patch/quiz.patch",
                "Remove Pop up form from edit question": "patches/quizquestion.patch",
				"Dynamic title for question answering form": "patch/issue_quiz_title.patch",
                "add Label for quiz page": "patch/quiz-page.patch",
                "Change textfield to textarea for short answer type questions" : "patch/quiz_textarea.patch"

			},
            "drupal/entity_embed": {
                "height width attributes": "patches/image_height_width.patch"
            },
			"drupal/redirect_after_login": {
                "reset password redirect": "patches/redirect_reset_pass.patch"
            }


		},
        "composer-exit-on-patch-failure": false,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
